{
    "manifest_version": "1",
    "type": "WebApp",
    "meta": {
      "name": "Win Forecast",
      "version": "0.0.0",
      "minimum-overwolf-version": "0.109.0.0",
      "author": "Overwolf",
      "icon": "Files/icons/IconMouseOver.png",
      "icon_gray": "Files/icons/IconMouseNormal.png",
      "launcher_icon": "Files/icons/launcher_icon.ico",
      "window_icon": "Files/icons/IconMouseOver.png",
      "description": "Your % chance to win"
    },
    "hide_from_store": true,
    "permissions": [
      "Hotkeys",
      "Media",
      "GameInfo",
      "Streaming",
      "FileSystem",
      "Profile",
      "Extensions"
    ],
    "data": {
      "extra-objects": {
        "simple-io-plugin": {
          "file": "simple-io-plugin.dll",
          "class": "overwolf.plugins.SimpleIOPlugin"
        }
      },
  
      "externally_connectable": {
        "matches": [
          "http://*.overwolf.com",
          "https://*.google-analytics.com",
          "http://*.google-analytics.com",
          "https://www.googleapis.com"
        ]
      },
      "game_targeting": {
        "type": "dedicated",
        "game_ids": [
            5426
        ]
      },
      "game_events": [5426],
  
      "launch_events": [
        {
          "event": "GameLaunch",
          "event_data": {
            "game_ids": [10902]
          },
          "start_minimized": false
        }
      ],
  
      "start_window": "desktop",
      "windows": {
        "main": {
          "file": "index.html",
          "in_game_only": true,
          "size": {
            "width": 250,
            "height": 250
          },
          "start_position": {
            "top": 0,
            "left": 0
          },
          "transparent": false,
          "clickthrough": true,
          "show_in_taskbar": true,
          "grab_keyboard_focus": false,
          "grab_focus_on_desktop": false,
          "background_optimization": true
        },
        "desktop": {
          "file": "desktop.html",
          "desktop_only": true,
          "native_window": true,
          "resizable": true,
          "transparent": false,
          "override_on_update": true,
          "size": {
            "width": 1212,
            "height": 699
          },
          "min_size": {
            "width": 1212,
            "height": 699
          }
        }
      }
    }
  }
